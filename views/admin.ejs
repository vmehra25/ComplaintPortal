<body class="w3-light-grey">
  <!-- Top container -->
  <div class="w3-bar w3-top w3-black w3-large" style="z-index:4">
      <button class="w3-bar-item w3-button w3-hide-large w3-hover-none w3-hover-text-light-grey" onclick="w3_open();"><i
          class="fa fa-bars"></i> Â Menu</button>
      <span class="w3-bar-item w3-right">
        <form action="/adminSignOut" method="POST">
          <button type="submit">Logout</button>
        </form>
      </span>
    </div>
    <br>
    <br>
    <br>
<div style="padding-left: 32px;">
<h1>Manage Complaints....</h1>
<div class="w3-container w3-padding-16" style="border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;">
          <div class="w3-white w3-padding-16" style="padding-left: 16px; padding-right: 16px; display: inline-block;">
          <p><b>Type:</b></p>
          <div style="display: flex;">
              <label style="padding-right : 1cm;" class=" apsy-container">
                <input class="w3-radio" type="radio" name="adminCat" value="general" checked>
                <span class="checkmark"></span>General</label>
                
              <label class="apsy-container">
                <input class="w3-radio" type="radio" name="adminCat" value="personal">
                <span class="checkmark"></span>Personal</label>
          </div>

          <p><b>Status:</b></p>
          <div style="display: flex;">
                <label style="padding-right : 1.2cm;" class=" apsy-container">
                  <input class="w3-radio" type="radio" name="adminStatus" value="solved" checked>
                  <span class="checkmark"></span> Solved</label>
                  
                <label style="padding-right : 1cm;" class="apsy-container">
                  <input class="w3-radio" type="radio" name="adminStatus" value="pending">
                  <span class="checkmark"></span>Pending</label>

                  <label class="apsy-container">
                        <input class="w3-radio" type="radio" name="adminStatus" value="rejected">
                        <span class="checkmark"></span>Rejected</label>
            </div>
          </div class="w3-padding-16" style="padding-left:16px; padding-right:16px">
            <div>
            <!-- Dropdown Menu -->
            <div style="display: flex;">
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
              <p style="padding-top: 5px; padding-bottom: 1px;"><b>Category :</b></p>
              <select id="adminSelect"  style="border-radius: 5px; " class="dropbtn" name="adminSelect">
                <option value="all" selected>All</option>
                <option value="hostel">Hostel</option>
                <option value="fees">Fees</option>
                <option value="mess">Mess</option>
                <option value="faculty">Faculty</option>
                <option value="lab">Lab</option>
                <option value="library">Library</option>
              </select>
            </div>
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
              <p style="padding-top: 5px; padding-bottom: 1px;"><b>From :</b></p>
            <input type="date" id="adminFromDate"  class="dropbtn">
            </div>
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
              <p style="padding-top: 5px; padding-bottom: 1px;"><b>To :</b></p>
            <input type="date" id="adminToDate" class="dropbtn">
            </div>

            <div style="padding-bottom: 10px; margin-right: 10px;;" >
                <p style="padding-top: 5px; padding-bottom: 1px;"> <br>  </p>
              <button id="adminSubmit" onclick="loadComplaints('admin');" class="dropbtn"><i class="fa fa-search"></i></button>
            </div>
          </div>
          <p class="w3-text-red" id="adminWarning"></p>

        </div>
        <p><i id="adminNumRes"></i></p>
           
                <div class="w3-twothird w3-padding-16 w3-white" style="padding-left: 16px; padding-right: 16px; border-radius:8px" >
            <table id='adminTable' class= 'w3-table  w3-striped w3-white' >
               
                <!-- <td><b>Category</b></td><td><b>Registered On</b></td><td><b>Description</b></td><td colspan="2"><b>Status</b></td></tr><tr><table class="w3-table"></table></tr> -->
            </table>
          </div>
      
      </div>

      <div id="AdminComplaints" class="w3-modal">
            <header class="w3-display-container w3-content w3-hide-small" style="max-width:1500px">
              <div class="w3-modal-content w3-animate-zoom" style="border-top-left-radius: 15px; border-top-right-radius: 15px; border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;" >
          
                <div class="w3-bar w3-black" style="border-top-left-radius: 15px; border-top-right-radius: 15px; " >
                  <span onclick="document.getElementById('AdminComplaints').style.display='none'"
                    class="w3-button w3-display-topright" style="border-top-right-radius: 15px;">x</span>
                  <button id="AdminBtn" class="w3-bar-item w3-button Admintablink"> <!-- onclick="openLink(event, 'Admin','Admintablink','AdminmyLink');">-->
                    <i
                      class="fas fa-campground"></i> Make Changes </button>
                </div>
                
                <div id="Admin" class="w3-container w3-padding-16 AdminmyLink" style="border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;">
                
                 
                <div style="display: flex;" >
                      <label style="padding-right : 1.2cm;" class=" apsy-container">
                        <input class="w3-radio" type="radio" name="adminChangeStatus" value="solved" checked>
                        <span class="checkmark"></span> Solved</label>
                        
                      <label style="padding-right : 1cm;" class="apsy-container">
                        <input class="w3-radio" type="radio" name="adminChangeStatus" value="pending">
                        <span class="checkmark"></span>Pending</label>
      
                        <label class="apsy-container">
                              <input class="w3-radio" type="radio" name="adminChangeStatus" value="rejected">
                              <span class="checkmark"></span>Rejected</label>
                  </div>
                  <div style="display: flex;">
                <input name="adminPersonAssigned"  style=" margin-right: 1cm;  ;background-color: white; border-width: 3px; border-color:#607D8B; border-radius: 5px;padding: 5px ;color: #607D8B;" placeholder="Assign a person" /> 
                    <button id="adminChange" class="dropbtn">Change</button>
              </div>
                </div>


            </div>

        </header>
      </div>
    </div>
    </body>

</html>