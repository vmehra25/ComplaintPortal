      <form method="POST" action="/createComplaint" id="hostelComplaintForm">
      <!-- Hostel Complaint Tab -->
      <div id="Hostel" class="w3-container w3-white w3-padding-16 myLink" style="border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;">

        <!-- Complaint general or personal -->
        <div style="display: flex;">
          <label style="padding-right: 1cm;" class="apsy-container">
            <input class="w3-radio" type="radio" name="general" value="general" checked>
            <span class="checkmark"></span>General</label>

          <label class="apsy-container">
            <input class="w3-radio" type="radio" name="general" value="personal">
            <span class="checkmark"></span>Personal</label>
        </div>


        <!-- Dropdown Menu -->
        <div style="padding-bottom: 10px;">
          <p style="padding-top: 5px; padding-bottom: 5px;">Related To :</p>
          <select id="hostelSelect" onchange="whatChecked(this,'hostelOther')" style="border-radius: 5px;"
            class="dropbtn" name="hostelSelect">
            <option value="electrical">Electrical</option>
            <option value="Water">Water</option>
            <option value="network">Network</option>
            <option value="other">Other</option>
          </select>
        </div>
        <!-- Other Input -->
        <input name="hostelOther" type="text" placeholder="Specify the Other"
          style="border-radius: 5px; visibility: hidden; padding: 5px;  ; color: black;">
          <input name="category" type="text" id='category' value="hostel" hidden="true">
        <!-- Description -->
        <div>
          <p>Description</p>
          <textarea maxlength="200" form="hostelComplaintForm" name="description" id="hostelDesc" rows="4" cols="50" placeholder="Description should not exceed 200 characters"
            style="border-radius: 5px;"></textarea>
        </div>
        <!-- Submit -->
        <button id="hostelSubmit" onclick="compSubmit('hostel')" class="dropbtn" type="submit">Submit</button>

      </div>

    </form>
