        </div>
  
        <!-- Tabs -->
  
        <!-- Hostel Complaint Tab -->
        <div id="Pending" class="w3-container w3-white w3-padding-16 PendingmyLink" style="border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;">
          
          
            
            <!-- General Or Personal -->
            <div style="display: flex;">
              <label style="padding-right: 1cm;" class="apsy-container">
                <input class="w3-radio" type="radio" name="pendingCat" value="general" checked>
                <span class="checkmark"></span>General</label>
    
              <label class="apsy-container">
                <input class="w3-radio" type="radio" name="pendingCat" value="personal">
                <span class="checkmark"></span>Personal</label>
            </div>
            
            <!-- Dropdown Menu -->
            <div style="display: flex;">
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
              <p style="padding-top: 5px; padding-bottom: 1px;">Category :</p>
              <select id="pendingSelect"  style="border-radius: 5px; " class="dropbtn" name="pendingSelect">
                <option value="all" selected>All</option>
                <option value="hostel">Hostel</option>
                <option value="fees">Fees</option>
                <option value="mess">Mess</option>
                <option value="faculty">Faculty</option>
                <option value="lab">Lab</option>
                <option value="library">Library</option>
              </select>
            </div>
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
              <p style="padding-top: 5px; padding-bottom: 1px;">From :</p>
            <input type="date" id="pendingFromDate"  class="dropbtn">
            </div>
            <div style="padding-bottom: 10px; margin-right: 10px;;" >
